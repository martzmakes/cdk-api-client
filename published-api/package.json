{
  "name": "@martzmakes/published-api",
  "version": "0.1.0",
  "bin": {
    "published-api": "bin/published-api.js"
  },
  "scripts": {
    "build": "npm run clean && npm run generateApi && echo Using TypeScript && tsc --version && tsc --pretty --project tsconfig-publish.json && npm run copy",
    "clean": "rm -rf cdk.out",
    "copy": "cp -r README.md generatedClient/",
    "watch": "tsc -w",
    "test": "jest",
    "cdk": "cdk",
    "generateApi": "npx @martzmakes/api-constructs generateInternalApiClient publishedApi lib/routes/internal.ts"
  },
  "devDependencies": {
    "@martzmakes/api-constructs": "../api-constructs/dist",
    "@types/aws-lambda": "^8.10.148",
    "@types/jest": "^29.5.14",
    "@types/node": "22.7.9",
    "aws-cdk": "2.1006.0",
    "esbuild": "^0.25.1",
    "jest": "^29.7.0",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.2",
    "typescript": "~5.6.3"
  },
  "dependencies": {
    "@aws-crypto/sha256-js": "^5.2.0",
    "@martzmakes/constructs": "^0.7.1",
    "@smithy/protocol-http": "^5.1.0",
    "@smithy/signature-v4": "^5.0.2",
    "aws-cdk-lib": "2.185.0",
    "constructs": "^10.0.0"
  }
}
